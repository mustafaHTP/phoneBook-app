@{
    ViewData["Title"] = "GetContactWithPhoneNumbers";
}
@model ContactWithPhoneNumbersViewModel

<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.9.1/font/bootstrap-icons.css">

<div class="container rounded p-5 my-3 bg-gradient bg-primary text-white">
    <p class="text-center h1">Phone Numbers</p>
</div>

<div class="container rounded p-1 my-1 bg-gradient bg-dark text-white">
    <p class="text-start h4">@Model.FullName</p>
</div>
<table class="phoneNumbersTable table table-striped table-hover table-bordered">
    <thead class="table-dark text-white">
        <tr>
            <td>Id</td>
            <td>Phone Numbers</td>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.PhoneNumbers)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.PhoneNo</td>
            </tr>
        }
    </tbody>

</table>